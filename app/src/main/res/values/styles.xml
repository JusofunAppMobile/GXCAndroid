<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/horizontal_slide</item>
    </style>

    <style name="MyTheme.Layout.Root" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:fitsSystemWindows">true</item>
    </style>

    <style name="MyTheme.Layout.Root2" parent="MyTheme.Layout.Root">
        <item name="android:fitsSystemWindows">false</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="horizontal_slide" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/slide_in_from_right</item>
        <item name="android:activityCloseExitAnimation">@anim/slide_out_to_right</item>

        <item name="android:activityOpenExitAnimation">@anim/slide_out_to_left</item>
        <item name="android:activityCloseEnterAnimation">@anim/slide_in_from_left</item>
    </style>

    <style name="dialogWindowAnim" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/to_top</item>
        <item name="android:windowExitAnimation">@anim/to_bottom</item>
    </style>



    <style name="vertical_slide" parent="@android:style/Animation.Activity">
        <!--<item name="android:activityOpenEnterAnimation">@anim/silde_in_to_top</item>-->
        <!--<item name="android:activityCloseExitAnimation">@anim/silde_out_to_bottom</item>-->
        <!--<item name="android:activityCloseEnterAnimation">@anim/silde_out_to_bottom</item>-->
        <item name="android:activityOpenExitAnimation">@anim/silse_top_bottom_stay</item>
        <item name="android:activityCloseEnterAnimation">@anim/silse_top_bottom_stay</item>
        <item name="android:activityOpenEnterAnimation">@anim/silde_in_to_top</item>
        <item name="android:activityCloseExitAnimation">@anim/silde_out_to_bottom</item>
    </style>

    <style name="my_style_horizontal" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowAnimationStyle">@style/horizontal_slide</item>
        <item name="windowNoTitle">true</item>
        <!--<item name="android:windowIsTranslucent">true</item>-->
        <item name="windowActionBar">false</item>
        <!--<item name="android:windowBackground">@color/white</item>-->
    </style>

    <style name="C_Splash" parent="my_style_horizontal">
        <item name="android:windowBackground">@drawable/layer_splash_bg</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:screenOrientation">portrait</item>
        <item name="android:configChanges">orientation|keyboardHidden|locale|navigation|screenSize
        </item>
    </style>

    <style name="style_fullscreen" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowAnimationStyle">@style/horizontal_slide</item>
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:windowIsTranslucent">true</item>-->
        <item name="windowActionBar">false</item>
        <!--<item name="android:windowBackground">@color/white</item>-->
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="my_style_vertical" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowAnimationStyle">@style/vertical_slide</item>
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:windowIsTranslucent">true</item>-->
        <item name="android:windowBackground">@color/white</item>
    </style>


    <!-- 自定义dialog-->
    <style name="my_dialog" parent="Theme.AppCompat.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


    <style name="tool_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <!-- 分享对话框样式 -->
    <style name="share_dialog_style" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/dialog_enter</item>
        <item name="android:windowExitAnimation">@anim/dialog_exit</item>
    </style>

    <style name="menu_window">
        <item name="android:windowEnterAnimation">@anim/in_top_bottom</item>
        <item name="android:windowExitAnimation">@anim/out_bottom_top</item>
    </style>

    <style name="recycle">
        <item name="android:listDivider">@color/background_material_dark</item>
    </style>

    <style name="xlistview_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:dividerHeight">1px</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:descendantFocusability">blocksDescendants</item>
        <item name="android:footerDividersEnabled">false</item>
    </style>

